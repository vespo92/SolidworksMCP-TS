# SolidWorks Configuration
# Copy this file to .env and update with your paths

# SolidWorks installation path (optional - will auto-detect if not set)
SOLIDWORKS_PATH=C:/Program Files/SOLIDWORKS Corp/SOLIDWORKS

# Default SolidWorks version (2021-2025)
SOLIDWORKS_VERSION=2024

# Default paths for models and macros
SOLIDWORKS_MODELS_PATH=C:/SolidWorks/Models
SOLIDWORKS_MACROS_PATH=C:/SolidWorks/Macros

# ChromaDB configuration (optional)
CHROMA_HOST=localhost
CHROMA_PORT=8000

# Logging level (debug, info, warn, error)
LOG_LEVEL=info